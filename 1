apiVersion: networking.k8s.io/v1
kind: NetworkPolicy
metadata:
  name: javawebappnetworkpolicy
  namespace: test-ns
spec:
  podSelector:
    matchLabels:
      app: javawebapp
  policyTypes:
  - Ingress
  ingress:
    - from:
      - podSelector:
          matchLabels:
            app: nginx
      ports:
      - protocol: TCP
        port: 80
